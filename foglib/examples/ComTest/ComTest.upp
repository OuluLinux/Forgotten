noblitz;

uses
	Com;

include
	.;

file
	Readme.txt,
	ComTest.mfog highlight cpp,
	"FOG Generated" readonly separator,
	Global.hxx,
	Global.cxx,
	Com.hxx,
	Com.cxx,
	C.hxx,
	C.cxx,
	_ readonly separator;

mainconfig
	"" = "";

custom(POSIX) "mfog",
	"fog -cd$(DIR) -t4 -I../../src -DPOSIX -DCPU64 -global Global $(PATH)\n",
	"$(DIR)/CoreTest.hxx";

custom(WIN32) "",
	"fog -cd$(DIR) -t4 -I../../src -DWIN32 -DCPU64 -global Global $(PATH)\n",
	"$(DIR)/CoreTest.hxx";

