
namespace Algorithm {

template <class T> const T& Minimum(const T& a, const T& b) {
	if (a < b)
		return a;
	else
		return b;
}

template <class T> const T& Maximum(const T& a, const T& b) {
	if (a >= b)
		return a;
	else
		return b;
}


}

#define Min		Algorithm::Minimum
#define Max		Algorithm::Maximum
